#include <iostream>
#include <vector>
using ivec = std::vector <int>;
void print(ivec v) {
  v[1] = 10;
  v.push_back(213);
  std::cout << "Element 1 is " << v[1]
	      << " size of v is " << v.size() << "\n"; 
}

int main () {
  ivec vi1 {2 ,5 ,4 ,7 ,9 ,2};
  print(vi1);
  std::cout << "Element 1 is " << vi1[1]
	    << " size of v is " << vi1.size() << "\n";
}
